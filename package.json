    "@vitejs/plugin-vue-jsx": "2.0.0",
    "axios": "^0.27.2",
    "husky": "^8.0.1",
    "lint-staged": "^11.2.6",
    "unocss": "0.44.5",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.7",
    "vite": "3.0.7",
    "vitepress": "0.22.4",
    "vitepress-theme-demoblock": "1.4.2"
